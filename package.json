{
  "name": "docker-db-manager",
  "private": true,
  "version": "1.0.3",
  "description": "Docker Database Manager - Multi-database management tool for macOS",
  "type": "module",
  "scripts": {
    "tauri": "tauri",
    "dev": "tauri dev",
    "build": "tauri build",
    "clean": "rm -rf node_modules src-tauri/target",
    "test": "cd src-tauri && cargo test --lib -- --nocapture --test-threads=1",
    "test:watch": "cd src-tauri && cargo watch -x 'test --lib -- --nocapture --test-threads=1'",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "format:check": "biome check ./src",
    "check": "biome check --write ./src"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.3",
    "@tauri-apps/cli": "^2"
  },
  "keywords": [
    "docker",
    "database",
    "postgresql",
    "mysql",
    "mongodb",
    "redis",
    "database-management",
    "macos",
    "tauri",
    "rust"
  ],
  "author": "alemelgarejo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alemelgarejo/docker-database-manager.git"
  },
  "dependencies": {
    "chart.js": "^4.5.1"
  }
}
